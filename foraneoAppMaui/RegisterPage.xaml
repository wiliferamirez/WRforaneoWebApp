<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="foraneoAppMaui.RegisterPage"
             Title="RegisterPage">

    <StackLayout>
        <Entry x:Name="EmailEntry" Placeholder="Email" />
        <Entry x:Name="PasswordEntry" Placeholder="Password" IsPassword="True" />
        <Entry x:Name="CedulaEntry" Placeholder="Cedula" />
        <Entry x:Name="FirstNameEntry" Placeholder="First Name" />
        <Entry x:Name="LastNameEntry" Placeholder="Last Name" />
        <Entry x:Name="ProvinceEntry" Placeholder="Province" />
        <Entry x:Name="ProgramEntry" Placeholder="Program" />
        <DatePicker x:Name="BirthDayPicker" />

        <Button Text="Register" Clicked="OnRegisterClicked" />
    </StackLayout>

</ContentPage>
